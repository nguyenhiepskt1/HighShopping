@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Người dùng";
}

<h1>Người dùng</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Tài khoản
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.UserName
            </td>
            <td>
                @item.Email
            </td>
        </tr>
}
    </tbody>
</table>
